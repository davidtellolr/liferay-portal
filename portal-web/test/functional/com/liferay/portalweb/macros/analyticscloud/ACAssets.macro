definition {

	macro viewAssets {
		var key_assetName = "${assetName}";

		AssertTextEquals(
			locator1 = "ACAssets#ASSETS_TABLE",
			value1 = "${assetName}");
	}

	macro viewVisitorsBehaviorMetric {
		var key_metricTitle = "${metricTitle}";

		AssertTextEquals(
			locator1 = "ACAssets#METRIC_VALUE",
			value1 = "${metricValue}");
	}

}